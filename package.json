{
	"name": "program-runner",
	"version": "0.1.0",
	"description": "A Library to run programs of any language in js",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsup"
	},
	"files": [
		"dist/**/*",
		"README.md"
	],
	"keywords": [
		"program-runner",
		"python in js",
		"java in js",
		"npm",
		"node",
		"nodejs",
		"typescript",
		"node program runner",
		"nodejs program runner"
	],
	"author": "Raman Sharma",
	"repository": {
		"type": "git",
		"url": "https://github.com/RamanSharma100/program-runner.git"
	},
	"license": "MIT",
	"eslintConfig": {
		"rules": {
			"import/no-anonymous-default-export": "off"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/node": "^20.11.16",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	}
}
